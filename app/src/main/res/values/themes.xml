<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ProjectAlgi001" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/color_three</item>
        <item name="colorPrimaryVariant">@color/color_one</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/color_three</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>

        <item name="windowActionModeOverlay">true</item>
        <!-- Customize your theme here. -->
    </style>

    <!-- Custom Color Algi -->
    <style name="colorLayoutProfile">
        <item name="background">@drawable/rounded_text_change_pass</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="colorCardviewDetailCar" parent="CardView.Light">
        <item name="cardBackgroundColor">@color/cardview_detail_car_day</item>
    </style>

    <style name="colorCardviewListCustomer">
        <item name="cardBackgroundColor">@color/cardview_detail_car_day</item>
    </style>

    <style name="colorTextDetailCar">
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="colorEditTextSpk">
        <item name="android:textColorHint">@color/white</item>
    </style>

    <style name="colorTextLoginPage">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="colorEditTextLoginPage">
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="colorToggle">
        <item name="passwordToggleTint">@color/color_one</item>
    </style>

    <style name="colorTitleChangeEmail">
        <item name="android:textColor">@color/color_one</item>
    </style>

    <style name="Theme.ProjectAlgi001.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.ProjectAlgi001.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.ProjectAlgi001.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>